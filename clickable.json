{
  "clickable_minimum_required": "6.24.0",
  "builder": "custom",
  "default": "clean build click-build",
  "kill": "code*",
  "build": "bash ${ROOT}/build.sh ${ARCH}",
  "dependencies_host": [
    "libgles2",
    "wget"
  ],
  "dependencies_target": [
    "libasound2",
    "libfreetype6",
    "libglib2.0-0",
    "libpng16-16",
    "libgtk-3-0",
    "libdbusmenu-glib4"
  ],
  "install_lib": [
    "/usr/lib/${ARCH_TRIPLET}/libgtk-3.so*"
  ]
}
